{
  "name": "yaas",
  "version": "0.0.1",
  "description": "Yet Another Angular2 Starter-kit (YAAS) featuring Webpack, Karma, Istanbul, Typescript, SASS",
  "keywords": [
    "angular2",
    "webpack",
    "typescript",
    "sass",
    "karma",
    "istanbul",
    "starter",
    "kit",
    "starter kit"
  ],
  "author": "Aakash Goel <se.aakashgoel@gmail.com>",
  "homepage": "https://github.com/areai51/YAAS",
  "license": "MIT",
  "engines": {
    "node": ">=5",
    "npm": ">=3"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run start:dev",
    "start:dev": "OPEN=true webpack-dev-server --config config/webpack.config.js --inline --progress",
    "start:prod": "ENV=prod OPEN=true webpack-dev-server --config config/webpack.config.js --inline --progress",
    "test": "npm run test:dev",
    "test:dev": "karma start config/karma.conf.js",
    "pretest:dev": "rimraf dist/reports",
    "test:prod": "ENV=prod karma start config/karma.conf.js",
    "pretest:prod": "rimraf dist/reports",
    "test-watch": "nodemon --watch src --ext '*' --exec 'npm run lint && npm run test'",
    "test-debug": "karma start config/karma-debug.conf.js",
    "lint": "npm run eslint && npm run tslint && npm run stylelint",
    "eslint": "eslint --fix --color --ignore-path .gitignore --ext .js .",
    "tslint": "tslint 'src/**/*.ts'",
    "stylelint": "stylelint 'src/**/*.scss' --syntax scss",
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.config.js --progress --profile --bail",
    "prebuild:dev": "npm run clean && npm run lint",
    "build:prod": "ENV=prod webpack --config config/webpack.config.js --progress --profile --bail",
    "prebuild:prod": "npm run clean && npm run lint",
    "postinstall": "typings install"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "autoprefixer": "6.3.7",
    "axios": "0.13.1",
    "babel-polyfill": "^6.22.0",
    "c3": "0.4.11",
    "comment-json": "1.1.3",
    "copy-webpack-plugin": "3.0.1",
    "core-js": "^2.4.0",
    "css-loader": "0.25.0",
    "eslint": "3.0.0",
    "eslint-loader": "1.4.1",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fs-extra": "0.30.0",
    "fs-node": "1.0.1",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "include-media": "1.4.8",
    "istanbul": "0.4.4",
    "istanbul-instrumenter-loader": "0.2.0",
    "json-loader": "0.5.4",
    "lodash": "4.13.1",
    "node-sass": "3.8.0",
    "nodemon": "1.9.2",
    "normalize.css": "^4.2.0",
    "open": "0.0.5",
    "parseuri": "0.0.4",
    "postcss-loader": "0.9.1",
    "progress-bar-webpack-plugin": "1.8.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.3",
    "rimraf": "2.5.3",
    "rxjs": "5.0.0-beta.6",
    "sass-loader": "4.0.0",
    "source-map-loader": "0.1.5",
    "stylelint": "7.1.0",
    "stylelint-config-standard": "12.0.0",
    "ts-helpers": "1.1.1",
    "ts-loader": "0.8.2",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.2",
    "typings": "1.3.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.14.0",
    "webpack-split-by-path": "https://github.com/patrickhousley/webpack-split-by-path.git#fedbfd4",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "codelyzer": "0.0.26",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-htmlfile-reporter": "0.3.4",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "1.1.0",
    "karma-mocha-reporter": "2.0.4",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-remap-istanbul": "https://github.com/bennett000/karma-remap-istanbul.git#98a5a04",
    "karma-source-map-support": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0"
  }
}
