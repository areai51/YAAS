{
    "extends": "stylelint-config-standard",
    "rules": {
        "at-rule-no-unknown": null,
        "at-rule-no-vendor-prefix": true,
        "block-closing-brace-space-after": null,
        "block-opening-brace-newline-before": null,
        "block-no-single-line": null,
        "color-no-hex": null,
        "color-named": "never",
        "comment-empty-line-before": ["always", {
            "except": ["first-nested"],
            "ignore": ["stylelint-commands", "between-comments"]
        }],
        "comment-word-blacklist": null,
        "custom-media-pattern": null,
        "custom-property-pattern": null,
        "custom-property-no-outside-root": true,
        "declaration-block-no-duplicate-properties": [true, {
            "ignore": ["consecutive-duplicates"]
        }],
        "declaration-block-properties-order": null,
        "declaration-block-semicolon-newline-before": null,
        "declaration-colon-newline-after": null,
        "declaration-colon-space-after": "always-single-line",
        "declaration-no-important": null,
        "font-family-name-quotes": "always-unless-keyword",
        "font-weight-notation": "numeric",
        "function-blacklist": ["filter"],
        "function-comma-newline-before": null,
        "function-url-data-uris": null,
        "function-url-quotes": "always",
        "function-whitelist": null,
        "indentation": "tab",
        "max-empty-lines": 2,
        "max-line-length": [ 200, {
            "ignore": ["non-comments"]
        }],
        "max-nesting-depth": 4,
        "media-feature-name-no-vendor-prefix": true,
        "no-browser-hacks": null,
        "no-descending-specificity": null,
        "no-duplicate-selectors": true,
        "no-empty-source": null,
        "no-indistinguishable-colors": [true, {
            "threshold": 1
        }],
        "no-unknown-animations": true,
        "no-unsupported-browser-features": null,
        "number-leading-zero": null,
        "number-max-precision": 5,
        "property-blacklist": null,
        "property-whitelist": null,
        "property-no-vendor-prefix": true,
        "declaration-property-unit-blacklist": {
            "font-size": ["px"]
        },
        "declaration-property-unit-whitelist": null,
        "declaration-property-value-blacklist": null,
        "declaration-property-value-whitelist": null,
        "root-no-standard-properties": true,
        "rule-nested-empty-line-before": ["always", {
            "except": ["first-nested"],
            "ignore": ["after-comment"]
        }],
        "selector-attribute-operator-blacklist": null,
        "selector-attribute-operator-whitelist": null,
        "selector-class-pattern": null,
        "selector-id-pattern": null,
        "selector-list-comma-newline-after": "always-multi-line",
        "selector-list-comma-newline-before": null,
        "selector-list-comma-space-after": "always-single-line",
        "selector-max-compound-selectors": 4,
        "selector-max-specificity": "0,6,0",
        "selector-no-attribute": null,
        "selector-no-combinator": null,
        "selector-no-id": true,
        "selector-no-qualifying-type": [true, {
            "ignore": [
                "attribute",
                "class"
            ]
        }],
        "selector-no-type": null,
        "selector-no-universal": true,
        "selector-no-vendor-prefix": null,
        "selector-root-no-composition": true,
        "selector-type-no-unknown": null,
        "shorthand-property-no-redundant-values": null,
        "string-quotes": null,
        "stylelint-disable-reason": "always-after",
        "time-no-imperceptible": null,
        "unit-blacklist": null,
        "unit-whitelist": null,
        "value-keyword-case": "lower",
        "value-list-comma-newline-before": "never-multi-line",
        "value-no-vendor-prefix": true
    }
}
